{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAdB2C": {
    "Instance": "https://<domain-name>.b2clogin.com",
    "ClientId": "<azure-api-application-client-id>",
    "Domain": "<domain-name>.onmicrosoft.com",
    "SignedOutCallbackPath": "/signout/<user-flow-name>",
    "SignUpSignInPolicyId": "<user-flow-name>"
    //"CallbackPath": "/signin/<user-flow-name>"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "https://localhost:44332"
      }
    }
  }
}
